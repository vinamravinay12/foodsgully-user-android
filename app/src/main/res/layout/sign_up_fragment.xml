<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.foodsgully.foodsgullyuser.fragments.SignUpFragment">

    <include
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:id="@+id/layout_email_password"
        layout="@layout/layout_username_password"
        android:visibility="visible"
        tools:visibility="gone"/>


    <include
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:id="@+id/layout_contact_details"
        layout="@layout/layout_signup_contact_details"
        android:visibility="gone"
        tools:visibility="gone"/>

    <include
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:id="@+id/layout_homeAddress"
        layout="@layout/layout_home_address"
        android:visibility="gone"
        tools:visibility="visible"/>


</androidx.constraintlayout.widget.ConstraintLayout>